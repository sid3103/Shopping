		<!-- HEADER -->
		<header>
			<!-- TOP HEADER -->
				<div id="top-header">
					<div class="container">
						<ul class="header-links pull-left">
							<li><a href="#"><i class="fa fa-user"></i> <%= current_user.name %></a></li>
							<li><a href="#"><i class="fa fa-envelope-o"></i><%= current_user.email %></a></li>
							<li><%= link_to "New Bike",new_two_wheeler_path %></li>
							<li><%= link_to "Upload Photo",new_photo_path %></li>
						</ul>
						<ul class="header-links pull-right">
							<!-- <li><a href="#"><i class="fa fa-dollar"></i> USD</a></li> -->
							<!-- <li><a><i class="fa fa-user-o"></i> My Account</a></li>						 -->
							<li><%= link_to 'Log Out', destroy_user_session_path, method: :delete, class: "fa fa-user-o " %></li>
						</ul>
					</div>
				</div>
			<!-- /TOP HEADER -->

			<!-- MAIN HEADER -->
			<div id="header">
				<!-- container -->
				<div class="container">
					<!-- row -->
					<div class="row">
						<!-- LOGO -->
						<div class="col-md-3">
							<div class="header-logo">
								<%= link_to root_path,class: "logo" do %>
								  <%= image_tag "/images/logo.png" %>
								<% end %>
							</div>
						</div>
						<!-- /LOGO -->

						<!-- SEARCH BAR -->
						<div class="col-md-6">
							<div class="header-search">
								<form>
									<select class="input-select">
										<option value="0">All Categories</option>
										<option value="1">Category 01</option>
										<option value="1">Category 02</option>
									</select>
									<input class="input" placeholder="Search here">
									<button class="search-btn">Search</button>
								</form>
							</div>
						</div>
						<!-- /SEARCH BAR -->

						<!-- ACCOUNT -->
						<div class="col-md-3 clearfix">
							<div class="header-ctn">
								<!-- Wishlist -->
								<div>
									<a href="#">
										<i class="fa fa-heart-o"></i>
										<span>Your Wishlist</span>
										<div class="qty">2</div>
									</a>
								</div>
								<!-- /Wishlist -->

								<!-- Cart -->
								<div class="dropdown">
									<a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
										<i class="fa fa-shopping-cart"></i>
										<span>Your Cart</span>
										<div class="qty"><%= current_user.cart.cart_items.count %></div>
										<%# binding.pry %>
									</a>
									<div class="cart-dropdown">
										<!-- CART ITEM LIST -->
										<% if @item_counts.present? && @cart.present? %>
										<%# binding.pry %>
											<% @cart.cart_items.each do |ci| %> 
												<div class="cart-list">
													<%# for product in current_user.cart.cart_items  %>
													<div class="product-widget">
														<div class="product-img">

															<!-- <img src="/images/product01.png" alt=""> -->
															<%= bike_dashboard_img(ci.two_wheeler) %>
														</div>
														<div class="product-body">
															<h3 class="product-name"><a href="#"><%= ci.two_wheeler.title %></a></h3>
															<h4 class="product-price"><span class="qty">1x</span>₹<%= ci.two_wheeler.price %></h4>
														</div>
														<!-- <button class="delete"><i class="fa fa-close"></i></button> -->
														<%= button_to 'X', cart_path(ci), class: 'delete', method: :delete, data: {confirm: 'Are you sure?'} %>
													</div>											
												</div>
											<% end %>
										<% end %>

										
										<!-- //////////CART ITEM LIST -->

										<!-- CART TOTAL AMOUNT -->										
										<div class="cart-summary">
											<small>3 Item(s) selected</small>
											<h5>SUBTOTAL: ₹123</h5>
										</div>
										<!-- /CART TOTAL AMOUNT -->

										<div class="cart-btns">
											<!-- <a href="#">View Cart <i class="view-cart"></i></a> -->
											<%= link_to "View Cart", carts_path %>
											<a href="#">Checkout  <i class="fa fa-arrow-circle-right Checkout"></i></a>											
										</div>							
									</div>
								</div>
								<!-- /Cart -->

								<!-- Menu Toogle -->
								<div class="menu-toggle">
									<a href="#">
										<i class="fa fa-bars"></i>
										<span>Menu</span>
									</a>
								</div>
								<!-- /Menu Toogle -->
							</div>
						</div>
						<!-- /ACCOUNT -->
					</div>
					<!-- row -->
				</div>
				<!-- container -->
			</div>
			<!-- /MAIN HEADER -->
		</header>
		<!-- /HEADER -->




